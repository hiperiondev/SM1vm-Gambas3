Public Sub Form_Open()
  Me.Title = "SM1 Load File"
  bLoadACCEPT.text = "ACCEPT"
  bLoadCANCEL.text = "CANCEL"
End

Public Sub bLoadACCEPT_Click()
  FMain.tbFILE.text = FileChooser1.SelectedPath
  FMain.file = FileChooser1.SelectedPath
  FMain.LoadFile
  load.Close
End

Public Sub bLoadCANCEL_Click()
  load.Close
End

